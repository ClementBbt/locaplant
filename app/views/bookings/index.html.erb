
<div class="card-category mb-5" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://images.unsplash.com/photo-1518607692857-bff9babd9d40?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1567&q=80")>
  <h1>Tous mes bookings</h1>
</div>
<div class="container d-flex justify-content-between">
<% @bookings.each do |booking| %>
<div class="card-booking">
  <%= cl_image_tag booking.plant.photo.key %>
  <div class="card-booking-infos">
    <h2><%= link_to booking.plant.name, plant_path(booking.plant) %></h2>
    <p><%= booking.start_date %></p>
    <p><%= booking.end_date %></p>
    <p><%= booking.total_price %></p>
  </div>
</div>
<% end %>
</div>





